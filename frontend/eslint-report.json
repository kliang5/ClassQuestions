e = props.sessionCode\n\n    fetch(`/api/class-session/${sessionCode}/question/${questionId}/upvote`,{\n      method: 'PUT',\n      headers:{\n        'Content-Type': 'application/json'\n      },\n    }).then(response=>{\n      if (response.ok){\n        console.log(\"Upvote question successful\");\n        console.log(props.question.upvotes);\n        props.onQuestionUpvoted()\n      }else{\n        console.log(\"Upvote question went wrong\")\n      }\n    })\n  };\n\n  return (\n    <div\n      style={{\n        borderTop: \"1px solid darkgrey\",\n        paddingTop: \"15px\",\n        paddingBottom: \"15px\",\n        display: \"flex\",\n      }}\n    >\n      <div>\n        <button\n          className=\"button\"\n          onClick={() => handleUpvote(props.question.id)}\n        >\n          <i className=\"material-icons\">recommend</i>\n        </button>\n      </div>\n      <div style={{ marginLeft: \"15px\" }}>\n        <b>{props.question.question}</b>\n        <br />\n        by {props.question.name}{\" \"}\n        <br />\n        {props.question.upvotes ? `Votes: ${props.question.upvotes}` : \"\"}\n      </div>\n      {props.isSignedIn && (\n        <div style={{ marginLeft: \"auto\" }}>\n          <button className=\"button is-danger\">Dismiss</button>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default ShowQuestion;\n","usedDeprecatedRules":[{"ruleId":"no-native-reassign","replacedBy":["no-global-assign"]},{"ruleId":"no-negated-in-lhs","replacedBy":["no-unsafe-negation"]}]},{"filePath":"/Users/nicoleliang/web-practice/milestone-2-kliang5/frontend/src/index.js","messages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[{"ruleId":"no-native-reassign","replacedBy":["no-global-assign"]},{"ruleId":"no-negated-in-lhs","replacedBy":["no-unsafe-negation"]}]},{"filePath":"/Users/nicoleliang/web-practice/milestone-2-kliang5/frontend/src/pages/ClassSessionPage.js","messages":